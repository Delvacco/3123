
#include <iostream>
using namespace std;
int main ()
{
int size;
cout << "Vvedite kol-vo elemenetov arr:";
cin >> size;
int arr[size];
        for (int i=0;i<size;i++ )
        {
            cout << "Vvedite chislo:";
            cin >> arr[i];
        }
        for (int i=size-1;i>=0;i--)
        {
            cout<<arr[i]<<"|";
        }
}
