
#include <iostream>
using namespace std;
int main ()
{
int n;
cout << "Vvedite kol-vo elemenetov arr:";
cin >> n;
int arr[n];
        for (int i=0;i<n;i++ )
        {
            cout << "Vvedite chislo:";
            cin >> arr[i];
        }
        for (int i=n-1;i>=0;i--)
        {
            cout<<arr[i]<<"|";
        }
}

