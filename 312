
#include <iostream>
using namespace std;
int main ()
{
int n;
std::cout << "Vvedite kol-vo elemenetov arr:";
std::cin >> n;
int arr[n];
        for (int i=0;i<n;i++ )
        {
            std::cout << "Vvedite chislo:";
            std::cin >> arr[i];
        }
        for (int i=n-1;i>=0;i--)
        {
            std::cout<<arr[i]<<"|";
        }
}

